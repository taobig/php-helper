{

	"name" : "taobig/php-helper",
	"description" : "A tool set for PHP projects",
	"type" : "library",
	"license" : "MIT",
	"minimum-stability" : "stable",
	"require" : {
		"php" : "^7.0",
		"php-64bit": "^7.0",
		"ext-bcmath":"*",
		"ext-json": "*",
		"guzzlehttp/guzzle": "^6.3 || ^8.4"
    },
	"require-dev" : {
		"aliyuncs/oss-sdk-php": "~2.0",
		"phpunit/phpunit": "^6.1 || ^9.0",
		"php-coveralls/php-coveralls": "^2.2"
	},
	"autoload" : {
		"psr-4" : {
			"taobig\\helpers\\" : "src/"
		},
		"files": ["preload.php"]
	},
	"homepage" : "https://github.com/taobig/php-helper",
	"support" : {
		"issues" : "https://github.com/taobig/php-helper/issues"
	}
}
